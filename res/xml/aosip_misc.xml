<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
    android:title="@string/misc_title" >

    <Preference
        android:title="@string/app_ops_settings"
        android:persistent="false"
        android:fragment="com.android.settings.applications.AppOpsSummary"/>

    <PreferenceScreen
        android:key="expanded_desktop"
        android:title="@string/expanded_desktop_category"
        android:fragment="com.android.settings.applications.ExpandedDesktopPreferenceFragment" />

    <PreferenceScreen
        android:key="density_changer"
        android:title="@string/lcd_density_title"
        android:fragment="com.android.settings.aosip.advanced.Density" />

</PreferenceScreen>

